{
  "name": "szemul/slim-request-handler",
  "description": "Request handler for slim",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "nesbot/carbon": "^2.0.0",
    "ramsey/uuid": "^3.0|^4.0",
    "szemul/not-set-value": "^2.0.0",
    "szemul/request-parameter-error-collector": "^1.0.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "jetbrains/phpstorm-attributes": "^1.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.4.0",
    "phpunit/phpunit": "^9.5.5",
    "szemul/slim-error-handler-bridge": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Szemul\\SlimRequestHandler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Szemul\\SlimRequestHandler\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": false
  }
}
